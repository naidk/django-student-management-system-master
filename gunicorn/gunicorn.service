[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/django-student-management-system-master
ExecStart=/home/ubuntu/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/django-student-management-system-master/student_management_system/student_management_system.sock student_management_system.wsgi:application

       
[Install]
WantedBy=multi-user.target